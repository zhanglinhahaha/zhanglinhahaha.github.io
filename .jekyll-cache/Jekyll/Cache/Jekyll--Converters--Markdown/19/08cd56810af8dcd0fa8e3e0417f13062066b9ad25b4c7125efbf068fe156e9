    stable_sort(words.begin(), words.end(),
                [](const string &amp;s1, const string &amp;s2){return s1.size() &lt; s2.size();});
    auto wc = partition(words.begin(), words.end(),
                [sz](string &amp;s){return s.size() &lt; sz;});
