    for(auto rbe = ive.crbegin(); rbe &lt; ive.crend(); ++rbe) {
        cout &lt;&lt; *rbe &lt;&lt; " ";
    }
    cout &lt;&lt; endl;
}

(10.35)
void test35() {
    ifstream fin("num.txt");
    istream_iterator&lt;int&gt; in(fin), eof;
    vector&lt;int&gt; ive(in, eof);
    for(auto end = ive.cend() - 1; end &gt;= ive.cbegin(); --end) {
        cout &lt;&lt; *end &lt;&lt; " ";
    }
    cout &lt;&lt; endl;
}

