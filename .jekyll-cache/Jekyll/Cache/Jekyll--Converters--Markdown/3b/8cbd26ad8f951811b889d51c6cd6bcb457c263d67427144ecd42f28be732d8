<div class="language-plaintext highlighter-rouge"><div class="highlight"><pre class="highlight"><code>(9.52)
#include &lt;iostream&gt;
#include &lt;stack&gt;
#include &lt;string&gt;

using namespace std;

bool lower(string l, string h) {
    if((h == "*" || h == "/") &amp;&amp; (l == "+" || l == "-")) {
        return true;
    }
    else if(l == "*" &amp;&amp; h == "/") {
        return true;
    }
    else if(l == "+" &amp;&amp; h == "-") {
        return true;
    }
    else return false;
}

void calcu(stack&lt;int&gt; &amp;v, stack&lt;string&gt; &amp;o) {
    int tmp1 = v.top();
    v.pop();
    int tmp2 = v.top();
    v.pop();
    int tmp;
    string ope = o.top();
    o.pop();
    if(ope == "+") {
        tmp = tmp1 + tmp2;
    }else if(ope == "-") {
        tmp = tmp2 - tmp1;
    }else if(ope == "*") {
        tmp = tmp1 * tmp2;
    }else if(ope == "/") {
        tmp = tmp2 / tmp1;
    }
    v.push(tmp);
}

void cal(string s) {
    auto begin = s.begin();
    stack&lt;string&gt; o;
    stack&lt;int&gt; v;
    while(s.size() != 0) {
        string ele = s.substr(0, s.find_first_of("+-*/()="));
        s = s.substr(s.find_first_of("+-*/()="));
        string ope = s.substr(0, 1);
        s.erase(s.begin());
        //æ“ä½œæ•°å…¥æ ˆ
        if(ele.size() != 0) {
            v.push(stoi(ele));
        }
        //æ“ä½œç¬¦å…¥æ ˆ
        if(ope.size() != 0 &amp;&amp; ope != "=") {
            if(!o.empty() &amp;&amp; lower(ope, o.top())) {
                calcu(v, o);
            }
            if(ope == ")") {
                string l = o.top();
                while(l != "(") {
                    calcu(v, o);
                    l = o.top();
                }
                o.pop();
            }
            else {
                o.push(ope);
            }
        }
    }
    while(!o.empty()) {
        calcu(v, o);
    }
    cout &lt;&lt; v.top() &lt;&lt; endl;
}

int main() {
    string s;
    while(getline(cin, s) &amp;&amp; s != "q") {
        if(*s.end() != '=') s.append("=");
        cal(s);
    }
    return 0;
}
