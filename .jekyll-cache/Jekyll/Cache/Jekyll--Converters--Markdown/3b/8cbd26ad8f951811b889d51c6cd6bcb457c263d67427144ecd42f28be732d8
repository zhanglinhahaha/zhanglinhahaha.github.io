    while(bilst != ilst.cend() &amp;&amp; bive != ive.cend()) {
        if(*bilst != *bive) return false;
        bilst++;
        bive++;
    }
    if(bilst == ilst.cend() &amp;&amp; bive == ive.cend()) return true;
    else return false;
}
