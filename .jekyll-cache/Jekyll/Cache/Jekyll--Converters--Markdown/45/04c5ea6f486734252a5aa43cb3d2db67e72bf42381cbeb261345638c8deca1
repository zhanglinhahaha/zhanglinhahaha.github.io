        <span class="kt">int</span> <span class="n">res</span> <span class="o">=</span> <span class="n">count_if</span><span class="p">(</span><span class="n">vi</span><span class="p">.</span><span class="n">begin</span><span class="p">(),</span> <span class="n">vi</span><span class="p">.</span><span class="n">end</span><span class="p">(),</span> <span class="n">bind2nd</span><span class="p">(</span><span class="n">modulus</span><span class="o">&lt;</span><span class="kt">int</span><span class="o">&gt;</span><span class="p">(),</span> <span class="n">num</span><span class="p">));</span>
        <span class="c1">//cout &lt;&lt; res &lt;&lt; endl;</span>
        <span class="k">if</span><span class="p">(</span><span class="n">res</span> <span class="o">==</span> <span class="mi">0</span><span class="p">)</span> <span class="p">{</span>
